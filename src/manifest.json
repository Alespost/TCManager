{
  "manifest_version": 2,
  "default_locale": "en",
  "name": "__MSG_extension_name__",
  "description": "__MSG_extension_description__",
  "version": "0.1",
  "author": "Ale≈° Postulka",
  "permissions": [
    "storage",
    "activeTab",
    "cookies",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "browser_action": {
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "node_modules/webextension-polyfill/dist/browser-polyfill.js",
      "common/common.js",
      "common/bit_field_conversion.js",
      "background_scripts/consent_string_creation.js",
      "background_scripts/background.js"
    ]
  },
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": true
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_idle",
      "js": [
        "node_modules/webextension-polyfill/dist/browser-polyfill.js",
        "content_scripts/inject_tcfapi_call.js",
        "content_scripts/create_consent_request.js",
        "content_scripts/message_listener.js"
      ]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "tcmanager@fitvut.com",
      "strict_min_version": "42.0"
    }
  }
}